# .env
# Configurazione per l'applicazione di test Telnyx Call Tracking

# Chiave API Telnyx (sostituisci con la tua chiave API)
TELNYX_API_KEY=YOUR_TELNYX_API_KEY

# Connection ID Telnyx per le chiamate (creato dal Mission Control di Telnyx)
TELNYX_CONNECTION_ID=YOUR_CONNECTION_ID

# URL base dell'applicazione per i webhook
APP_URL=https://your-app-url.com

# Porta del server (facoltativo, default 3000)
PORT=3000
